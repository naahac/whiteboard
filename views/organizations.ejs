        <a href="#" data-activates="mobile-demo" class="button-collapse"><i class="material-icons">menu</i></a>

        <ul class="side-nav" id="mobile-demo" style="transform: translateX(-100%);">
            <li><a href="sass.html">Sass</a></li>
            <li><a href="badges.html">Components</a></li>
            <li><a href="collapsible.html">JavaScript</a></li>
        </ul>
        <div class="row" style="height:100%; margin-bottom:0;">
            <div class="col s4 m4" style="line-height: 48px;"><img class="brand-logo" src="images/brainstorm.png"
                                                                   style="height:40px; padding-top:10px;"></div>
            <div id="nav-mobile" class="col s4 m4 right hide-on-med-and-down" style="height:100%; line-height:100%;">
                <ul class="tabs">
                    <li class="tab col s3"><a id="text_hover" href="#wubbalubba"><i style="padding-bottom:30px;" class="material-icons">collections</i></a></li>
                    <li class="tab col s3"><a id="text_hover" class="active" href="#wubbalubba"> <i class="material-icons big center">color_lens</i> </a></li>
                    <li class="tab col s3"><a id="text_hover disconnect" href="#wubbalubba"><i class="material-icons big center">person</i> </a></li>
                </ul>
            </div>
        </div>
    </div>
